extends layout
 
block content
	div#scheduler_here.dhx_cal_container(style = 'width:100%; height:100%;')
		div.dhx_cal_navline
			div.dhx_cal_prev_button
				&nbsp;
			div.dhx_cal_next_button
				&nbsp;
			div.dhx_cal_today_button
			div.dhx_cal_date
			div#dhx_minical_icon.dhx_minical_icon(onclick = 'show_minical()')
			div.dhx_cal_tab(name = 'day_tab', style="right:204px;")
			div.dhx_cal_tab(name = 'week_tab', style="right:140px;")
			div.dhx_cal_tab(name = 'month_tab', style="right:76px;")
		div.dhx_cal_header
		div.dhx_cal_data